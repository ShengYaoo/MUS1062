@model IEnumerable<YC.Models.OpenData>

@{
    ViewBag.Title = "View";
}

<h2>View</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.資料集名稱)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.服務分類)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.資料集描述)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DisplaySqe)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.資料集名稱)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.服務分類)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.資料集描述)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DisplaySqe)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
